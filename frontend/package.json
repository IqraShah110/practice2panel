{
  "name": "practice2panel-platform",
  "version": "1.0.0",
  "description": "Practice2Panel - AI Powered Interview Preparation Platform with skill practice and voice-to-voice mock interviews",
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-backend": "cd ../Backend && python start_server.py",
    "setup-backend": "cd ../Backend && pip install -r requirements.txt && python create_sample_tables.py",
    "dev": "concurrently \"npm run start-backend\" \"npm start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
